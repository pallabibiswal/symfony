{% extends "@FOSUser/layout.html.twig" %}

{% block body %}
    {{ parent() }}

    <div class="container col-md-12 mt-5">
        <div class="row">
            <div class="col-md-12 mt-3">
                <ul class="list-unstyled">
                    {% for info in data %}
                        {{ dump(info) }}
                        <li class="media">
                            <img class="mr-3" height="100px" width="100px" src="{{ asset('uploads/photos/') }}{{ info.file }}" alt="Generic placeholder image">
                            <div class="media-body">
                                <h2> Username : {{ info.username }}</h2>
                                <h4> Email Id : {{ info.email }}</h4>
                            </div>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}